doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible" content="IE=edge")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        title #{ title }

    body


        h1 Local Business

        form(method='post', enctype='multipart/form-data')
        input#imageUpload(type='file', name='imageUpload')
        label(for='OwnersName')  Owners Name:
        input#OwnersName(type='text', name='OwnersName')
        label(for='BusinessName')  Business Name:
        input#BusinessName(type='text', name='BusinessName')
        label(for='BusinessType')  Business Type:
        input#BusinessType(type='text', name='BusinessType')
        label(for='BusinessDesc')  Business Desc:
        input#BusinessDesc(type='text', name='BusinessDesc')
        input(type='submit', value='Submit')